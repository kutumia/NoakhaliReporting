<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>কৃষি যন্ত্রপাতি বিতরণ প্রতিবেদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/machineryFormPost" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                        
                                    <input type="hidden" class="form-control" name="district" placeholder="জেলার নাম" value="<%=dds%>">
                                    <input type="hidden" class="form-control" name="upazilla" placeholder="উপজেলার নাম" value="<%=upazillas%>">
                                    

                                    <div class="form-group">
                                        <label for="machine" class="col-md-3 control-label">কৃষি যন্ত্রপাতির নাম </label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="machine" placeholder="কৃষি যন্ত্রপাতির নাম " >
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="shongothon" class="col-md-3 control-label">কৃষক সংগঠনের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="shongothon" placeholder="কৃষক সংগঠনের নাম" >
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="number" class="col-md-3 control-label">কৃষি যন্ত্রপাতির সংখ্যা </label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="number" placeholder="কৃষি যন্ত্রপাতির সংখ্যা ">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="farmer" class="col-md-3 control-label">উপকারভোগী কৃষকের/কৃষাণির নাম ও পিতা/স্বামীর নাম </label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="farmer" placeholder="উপকারভোগী কৃষকের/কৃষাণির নাম ও পিতা/স্বামীর নাম ">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="village" class="col-md-3 control-label">গ্রাম/ব্লক </label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="village" placeholder="গ্রাম/ব্লক ">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="mobile" class="col-md-3 control-label">মোবাইল নম্বর</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="mobile" placeholder="মোবাইল নম্বর ">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="bitoron" class="col-md-3 control-label">বিতরণের তারিখ </label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="bitoron" placeholder="বিতরণের তারিখ ">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <label for="year">বছর :</label>
                                        <select id="year" name="year" required>
                                            <option value="2017">২০১৭-১৮</option>
                                            <option value="2018">২০১৮-১৯</option>
                                            <option value="2019">২০১৯-২০</option>
                                            <option value="2020">২০২০-২১</option>
                                            <option value="2021">২০২১-২২</option>
                                            <option value="2022">২০২২-২৩</option>
                                        </select>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>