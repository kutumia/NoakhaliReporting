<%- include('../../includes/header')-%>
        <div class="wrapper">
          <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
              <%- include('../../includes/sidebar')-%>
              <div class="main-content">
                <div class="container-fluid">
                    <div class="row clearfix">
                    <div id="signupbox" style="margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                        <div class="panel panel-info">
                            <div class="panel-heading">
                                <div class="panel-title"><h2>প্রদর্শনী স্থাপনের প্রাথমিক প্রতিবেদন ফর্ম</h2></div>
                                <p><%
                                if(success !=''){ %>
                                <%= success %>
                                    <% } %>
                                </p>
                                </div>  
                            <div class="panel-body" >
                                <form action="/upazilla/initialTrialFormPost/<%= records.id %>" id="FieldTrialForm" class="form-horizontal" method="post"  role="form">
                                    <div id="signupalert" style="display:none" class="alert alert-danger">
                                        <p>Error:</p>
                                        <span></span>
                                    </div>

                                    <div class="form-group">
                                        <label for="breedname" class="col-md-3 control-label">ব্যবহৃত জাতের নাম</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="breedname" placeholder="ব্যবহৃত জাতের নাম" value="<%=records.breedname%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="germinationRate" class="col-md-3 control-label">বীজ অংকুরোদগমের হার</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="germinationRate" placeholder="বীজ অংকুরোদগমের হার" value="<%=records.germinationRate%>">
                                        </div>
                                    </div>


                                    <div class="form-group">
                                        <label for="trialdate" class="col-md-3 control-label">প্রদর্শনী স্থাপনের তারিখ</label>
                                        <div class="col-md-9">
                                            <input type="date" class="form-control" name="trialdate" placeholder="প্রদর্শনী স্থাপনের তারিখ" value="<%=records.trialdate%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="present" class="col-md-3 control-label">প্রদর্শনীর বর্তমান অবস্থা</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="present" placeholder="প্রদর্শনীর বর্তমান অবস্থা" value="<%=records.present%>">
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="kphone" class="col-md-3 control-label">দায়িত্বপ্রাপ্ত উপসহকারী কৃষি কর্মকর্তার নাম ও মোবাইল নং</label>
                                        <div class="col-md-9">
                                            <input type="text" class="form-control" name="kphone" placeholder="দায়িত্বপ্রাপ্ত উপসহকারী কৃষি কর্মকর্তার নাম ও মোবাইল নং" value="<%=records.kphone%>">
                                        </div>
                                    </div>

                                   

                                    <div class="form-group">
                                        <div class="col-md-9">
                                            <input type="hidden" class="form-control" name="user_id" placeholder="id" value="<%= user_id %>">
                                        </div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <!-- Button -->                                        
                                        <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">   
                                        </div>
                                    </div>
                                    
                                    
                                        
                                    
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<%- include('../../includes/footer')-%>