<%if (records.length >0){ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
    <thead>
    <tr>
        <th>ক্রমিক নং</th>
        <th>জেলার নাম</th>
        <th>উপজেলার নাম</th>
        <th>কৃষকের/কৃষাণির নাম ও পিতা/স্বামীর নাম</th>
        <th>জাতীয় পরিচয়পত্র নম্বর</th>
        <th>গ্রাম/ব্লক</th>
        <th>মোবাইল নম্বর</th>
        <th>উদ্বোধণকারীর নাম ও পদবী</th>
        <th>ভ্রমণকৃত স্থানের নাম ও বর্ণনা</th>
        <th>উপজেলা কৃষি অফিসারের মন্তব্য</th>
        <th>উপ-পরিচালকের মন্তব্য</th>
        <th>প্রকল্প পরিচালকের মন্তব্য</th>
        <th>মন্তব্য করুন</th>


    </tr>
    </thead>
    <tbody>
        <% records.forEach(function(row,index){ %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= row.district %></td>
                <td><%= row.upazilla %></td>
                <td><%= row.name %></td>
                <td><%= row.nid %></td>
                <td><%= row.village%></td>
                <td><%= row.mobile %></td>
                <td><%= row.comment %></td>
                <td><%= row.podobi %></td>
                <td><%= row.bornona %></td>
                <td><%= row.ddComment %></td>
                <td><%= row.pdComment %></td>
                <td><a class="btn btn-primary btn-xs" href="/dd/motivationEdit/<%= row.id %>"><i class="fa fa-edit"></i></a></td>
              
            </tr>
        <% }) %>
    </tbody>
</table>
<% }else{ %>
